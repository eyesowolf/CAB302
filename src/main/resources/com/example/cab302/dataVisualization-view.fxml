<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.cab302.DataVisualizationController">
    <Button text="Load Data" onAction="#loadData"/>
    <LineChart fx:id="lineChart" prefHeight="400" prefWidth="600">
        <xAxis>
            <CategoryAxis label="Date"/>
        </xAxis>
        <yAxis>
            <NumberAxis label="Mood"/>
        </yAxis>
    </LineChart>
</VBox>